pipeline {
    agent { label 'maven-label' }

    tools {
        maven "maven-3.6.2"
    }

    stages {
         stage('clone') {
            steps {
                 checkout scm  
                  echo "build"
            }
        }
        stage('Build') {
            steps {
                 
                  echo "build"
            }
        }
         stage('deploy') {
            steps {
                  echo "deploy"
            }
        }
    }
}
